<main>
  <div class="row">  
    <div class="col-sm-2">
        
          
    </div>
    <div class="col-sm-10">
      <div class="w3-container w3-teal">
        <h1><%= the_username %> - myprofile photo</h1>
      </div> <!-- <div class="w3-container w3-teal"> -->
    
       <div class="main">
          <div class="col-md-6 col-sm-12">
              <div class="login-form">
                <form action="/upload/<%= the_user_id %>" method="post" enctype="multipart/form-data">

                  <div class="form-group">
                      <label hidden >User_id:</label>
                      <input hidden type="text" class="form-control" placeholder="user_id" name="user_id" id="user_id" value="<%= the_user_id %>">
                  </div>
                  <div>
                    <input type="file" accept="image/*" name="photo" >
                    <input type="submit" value="upload">
                  </div>
                  
                </form>
              
              </div> <!--  div class="login-form">-->    <!-- script below highlights date-->
    
            </div> <!-- <div class="col-md-6 col-sm-12">-->
      </div>  <!-- <div class="main"> -->
    </div>  <!-- <div class="col-sm-10">-->
  </div>   <!-- <div class="row">-->
  </main>


