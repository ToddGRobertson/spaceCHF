        <p align="center">page 1</p>
        
       <div class="main">
         <!--  col-md-12 displays four images on medium screen-->
          <div class="col-md-12 col-sm-12">
              <%= counter = 0 %>

              <!-- start at i=1 -> photo 1  for 2 users this shows 2 users-->
              <div > <!-- <div class="form-group"> -->
                  <div class="container-fluid">  <!-- <div class="container">-->
                      <div class="row no-gutters">  <!-- no gutters closes space between images-->
                        <% for (let i = 0; i < the_data_of_users.length; i=i+1) {  %> <!-- 20 profile displayed per page-->
                        <div class="col-sm">  <!-- <div class="col-sm">-->
                          <!-- 
                          <p><%= i %></p>
                          <p><%= the_data_of_users.length %></p>  -->
                          <!-- <% if( i < the_data_of_users.length ){ %> -->
                            <!-- form group adds padding to display -->
                          <div  class="container div1">
                            <!-- img class b helps keep images inline -->
                            <!-- <a href="../theirprofile/user/<%=the_user_id%>/receiver/<%= the_data_of_users[i].user_id %>" ><img class="b" src="../<%= the_data_of_users[i].image %>" alt="Girl in a jacket3" ></a>   -->
                            <a  href="../../myprofiletheirprofile/<%=the_user_id%>/<%= the_data_of_users[counter + i].user_id %>" >
                            <img class="b" src="../<%= the_data_of_users[counter + i].image %>" alt="Girl in a jacket3" ></a>
                            <!--<span class="above-above-bottom-left"><B><%= the_data_of_users[counter + i].username %></B></span>-->
