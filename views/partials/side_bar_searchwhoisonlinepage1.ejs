                            <div class="above-bottom-left"><B><%= the_data_of_users[counter + i].patient_or_caregiver %></B></div>
                            <div class="bottom-left"><B><%= the_data_of_users[counter + i].city %></B></div>
                            <div class="above-bottom-right"><B><%= the_data_of_users[counter + i].illness %></B></div>
                            <div class="bottom-right"><B><%= the_data_of_users[counter + i].state %></B></div>
                            <!--<div align="center" class="div3" > -->
                            <%= console.log(" 109 the_data_of_users[i].online " + the_data_of_users[counter + i].online ) %>
                            <% if( the_data_of_users[counter + i].online === false )   { %>
                              <div class="bottom-right" ><span class="black_dot"></span></div>
                            <% }else{ %>
                              <div class="bottom-right" ><span class="green_dot"></span></div>
                            <% }    %>  
                          </div>
                          
                          <!-- <% } %> -->
                        </div> <!-- <div class="col-sm">-->
                        <% } %> 
                      </div>   <!-- <div class="row"> -->      
                      
                  </div> <!-- class="container-fluid" -->
                 
            </div> <!-- <div class="col-md-6 col-sm-12">-->
            
      </div>  <!-- <div class="main"> -->
    </div>  <!-- <div class="col-sm-6"> -->
  </div>   <!-- <div class="row">-->
