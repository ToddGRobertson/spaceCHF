      <div class="col-sm-10">
        <div class="w3-container w3-teal">
          <h1><%= the_username %> myprofile read messages</h1>
        </div> <!-- <div class="w3-container w3-teal"> -->
      
         <div class="main">
            <div class="col-md-6 col-sm-12">
                <div class="login-form">
    
                  
                    <div class="form-group">
                      <label hidden >User_id:</label>
                      <input hidden type="text" class="form-control" placeholder="user_id" name="user_id" id="user_id" value="<%= the_user_id %>">
                    </div>
                    
                      <div class="form-group">
                        <table border="1">
                          <tr>
                            <td>
                              <label>Photo:</label>
                            </td>
                          </tr>
                          <tr>
                              <td>
                                <img src='../<%= the_image %>' alt="Michael" width="100" height="100"> 
                              </td>
                          </tr>
                          <tr>
                            <td>
                              <label>Date:</label>
                            </td>
                          </tr>
                          <tr>
                            <td>
                              <input type="text" class="form-control" placeholder="date" name="date" id="date" value='<%= the_date %>' >
                            </td>
                          </tr>
                          <tr>
                            <td>
                              <label>Subject:</label>
                            </td>
                          </tr>
                          <tr>
                            <td>
                              <input type="text" class="form-control" placeholder="subject" name="subject" id="subject" value='<%= the_subject %>' >
                            </td>
                          </tr>
                          <tr>
                            <td>
                              <label>Message:</label>
                            </td>
                          </tr>
                          <tr>
                            <td>
                              <textarea type="text" class="form-control" placeholder="message" name="message" id="message" ><%= the_message %></textarea>
                            </td>
                      
                          </tr>
                          <tr>
                            <td>
                              <textarea type="text" class="form-control" placeholder="message_id" name="message_id" id="message_id" ><%= the_message_id %></textarea>
                            </td>
                      
                          </tr>
                          <tr>
                            <td>
                              <a href="../../myprofilemessagedelete/<%= the_user_id %>/<%= the_message_id %>" >Delete message</a>
                            </td>
                          </tr>
                        </table>
                      
                      </div> <!-- <div class="form-group">-->
                              
              
                    
                    
                </div> <!--  div class="login-form">-->    <!-- script below highlights date-->
      
